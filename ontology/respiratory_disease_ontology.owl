@prefix ex:<http://example.com/schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Classes
ex:Disease a rdf:Class .
ex:Symptom a rdf:Class .
ex:Treatment a rdf:Class .

# Properties
ex:hasSymptom a rdf:Property ;
    rdf:domain ex:Disease ;
    rdf:range ex:Symptom .

ex:relatedTo a rdf:Property ;
    rdf:domain ex:Symptom ;
    rdf:range ex:Symptom .

ex:hasTreatment a rdf:Property ;
    rdf:domain ex:Disease ;
    rdf:range ex:Treatment .

# Triệu chứng
ex:Fever a ex:Symptom ;
    rdfs:label "Fever" .

ex:Cough a ex:Symptom ;
    rdfs:label "Cough" .

ex:Fatigue a ex:Symptom ;
    rdfs:label "Fatigue" .

ex:SoreThroat a ex:Symptom ;
    rdfs:label "Sore Throat" .

ex:Headache a ex:Symptom ;
    rdfs:label "Headache" .

ex:ChestPain a ex:Symptom ;
    rdfs:label "Chest Pain" .

ex:Wheezing a ex:Symptom ;
    rdfs:label "Wheezing" .

ex:ShortnessOfBreath a ex:Symptom ;
    rdfs:label "Shortness of Breath" .

# Các bệnh và triệu chứng liên quan
ex:Flu a ex:Disease ;
    rdfs:label "Flu" ;
    ex:hasSymptom ex:Fever, ex:Cough, ex:Fatigue, ex:Headache, ex:SoreThroat .

ex:Pneumonia a ex:Disease ;
    rdfs:label "Pneumonia" ;
    ex:hasSymptom ex:Fever, ex:Cough, ex:ShortnessOfBreath, ex:ChestPain .

ex:Asthma a ex:Disease ;
    rdfs:label "Asthma" ;
    ex:hasSymptom ex:Wheezing, ex:Cough, ex:ShortnessOfBreath, ex:ChestPain .

ex:Bronchitis a ex:Disease ;
    rdfs:label "Bronchitis" ;
    ex:hasSymptom ex:Cough, ex:Fever, ex:SoreThroat, ex:Fatigue .

ex:COVID19 a ex:Disease ;
    rdfs:label "COVID-19" ;
    ex:hasSymptom ex:Fever, ex:Cough, ex:SoreThroat, ex:ShortnessOfBreath .

# Mối quan hệ triệu chứng liên quan
ex:Cough ex:relatedTo ex:Fatigue ;
         ex:relatedTo ex:SoreThroat ;
         ex:relatedTo ex:ShortnessOfBreath .

ex:Fever ex:relatedTo ex:Fatigue ;
        ex:relatedTo ex:Headache .

ex:Wheezing ex:relatedTo ex:ShortnessOfBreath ;
           ex:relatedTo ex:ChestPain .

# Các phương pháp điều trị
ex:AntiviralMedication a ex:Treatment ;
    rdfs:label "Antiviral Medication" .

ex:PainRelievers a ex:Treatment ;
    rdfs:label "Pain Relievers" .

ex:OxygenTherapy a ex:Treatment ;
    rdfs:label "Oxygen Therapy" .

ex:Inhalers a ex:Treatment ;
    rdfs:label "Inhalers" .

ex:RestAndHydration a ex:Treatment ;
    rdfs:label "Rest and Hydration" .

ex:Vaccination a ex:Treatment ;
    rdfs:label "Vaccination" .

# Các phương pháp điều trị cho bệnh
ex:Flu ex:hasTreatment ex:AntiviralMedication, ex:PainRelievers, ex:RestAndHydration .
ex:Pneumonia ex:hasTreatment ex:OxygenTherapy, ex:AntiviralMedication .
ex:Asthma ex:hasTreatment ex:Inhalers, ex:RestAndHydration .
ex:Bronchitis ex:hasTreatment ex:PainRelievers, ex:RestAndHydration .
ex:COVID19 ex:hasTreatment ex:OxygenTherapy, ex:AntiviralMedication, ex:RestAndHydration, ex:Vaccination .
